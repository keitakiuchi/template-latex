% example.bib - 汎用的な参考文献例
% このファイルをコピーして、プロジェクトに合わせてカスタマイズしてください

% 基本的な論文の例
@article{example2024,
  title     = {Example Research Paper Title},
  author    = {Example Author and Another Author},
  journal   = {Journal of Example Research},
  volume    = {1},
  number    = {1},
  pages     = {1--10},
  year      = {2024},
  publisher = {Example Publisher}
}

% 書籍の例
@book{examplebook2024,
  title     = {Example Book Title},
  author    = {Book Author},
  year      = {2024},
  publisher = {Example Publisher},
  address   = {Example City}
}

% LaTeX関連の文献（intro.texで引用されているもの）
@book{latex_companion,
  title     = {The {LaTeX} Companion},
  author    = {Mittelbach, Frank and Goossens, Michel and Braams, Johannes and Carlisle, David and Rowley, Chris},
  edition   = {2},
  year      = {2004},
  publisher = {Addison-Wesley},
  address   = {Boston}
}

@book{knuth,
  title     = {The {TeX}book},
  author    = {Knuth, Donald E.},
  year      = {1984},
  publisher = {Addison-Wesley},
  address   = {Reading, Massachusetts}
}

@misc{latex_project,
  title   = {The {LaTeX} Project},
  author  = {{LaTeX} Project Team},
  url     = {https://www.latex-project.org/},
  year    = {2024},
  note    = {Accessed: 2024-01-01}
}

@book{japanese_latex_guide,
  title     = {日本語{LaTeX}入門},
  author    = {日本語{LaTeX}研究会},
  year      = {2020},
  publisher = {技術評論社},
  address   = {東京}
}

% 会議論文の例
@inproceedings{exampleconf2024,
  title        = {Example Conference Paper Title},
  author       = {Conference Author},
  booktitle    = {Proceedings of Example Conference},
  pages        = {100--110},
  year         = {2024},
  organization = {Example Organization}
}

% オンラインリソースの例
@online{exampleweb2024,
  title   = {Example Web Resource},
  author  = {Web Author},
  url     = {https://example.com},
  year    = {2024},
  urldate = {2024-01-01}
}

% 技術レポートの例
@techreport{examplereport2024,
  title       = {Example Technical Report},
  author      = {Report Author},
  institution = {Example Institution},
  year        = {2024},
  number      = {TR-2024-001}
}
